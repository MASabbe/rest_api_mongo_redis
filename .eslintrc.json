{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "google",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": "off",
        "new-cap": "off",
        "require-jsdoc": "off",
        "valid-jsdoc": "off"
    }
}
